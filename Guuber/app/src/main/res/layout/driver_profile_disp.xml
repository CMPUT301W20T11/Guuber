<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#010C0C"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".DriverProfilActivity">

    <ImageView
        android:id="@+id/imageViewDrIn"
        android:layout_width="wrap_content"
        android:layout_height="132dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="125dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/guuber" />

    <ImageView
        android:id="@+id/likeButtonDrIn"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="63dp"
        app:layout_constraintEnd_toStartOf="@+id/dislikeButtonDrIn"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDrIn"
        app:srcCompat="@drawable/guuber" />

    <ImageView
        android:id="@+id/dislikeButtonDrIn"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDrIn"
        app:srcCompat="@drawable/guuber" />

    <Button
        android:id="@+id/deleteAccButtonDrIn"
        android:layout_width="203dp"
        android:layout_height="41dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="18dp"
        android:text="DELETE ACCOUNT"
        android:textColor="#FF0000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/carRegLabelDrIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="14dp"
        android:text="Vehicle Registration"
        android:textColor="#FF5722"
        app:layout_constraintEnd_toStartOf="@+id/carRegTextDrIn"
        app:layout_constraintTop_toBottomOf="@+id/likeButtonDrIn" />

    <TextView
        android:id="@+id/usernameLabelDrIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="15dp"
        android:text="Username"
        android:textColor="#FF5722"
        app:layout_constraintEnd_toStartOf="@+id/usernameTextDrIn"
        app:layout_constraintTop_toBottomOf="@+id/carRegLabelDrIn" />

    <TextView
        android:id="@+id/phoneLabelDrIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="39dp"
        android:text="Phone"
        android:textColor="#FF5722"
        app:layout_constraintEnd_toStartOf="@+id/phoneTextDrIn"
        app:layout_constraintTop_toBottomOf="@+id/usernameLabelDrIn" />

    <TextView
        android:id="@+id/emailLabelDrIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="43dp"
        android:text="Email"
        android:textColor="#FF5722"
        app:layout_constraintEnd_toStartOf="@+id/emailTextDrIn"
        app:layout_constraintTop_toBottomOf="@+id/phoneLabelDrIn" />

    <TextView
        android:id="@+id/availabilityLabelDrIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="38dp"
        android:text="Status"
        android:textColor="#FF5722"
        app:layout_constraintEnd_toStartOf="@+id/availabilityTextDrIn"
        app:layout_constraintTop_toBottomOf="@+id/emailLabelDrIn" />

    <TextView
        android:id="@+id/carRegTextDrIn"
        android:layout_width="160dp"
        android:layout_height="20dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="51dp"
        android:ems="10"
        android:text="Name"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dislikeButtonDrIn" />

    <TextView
        android:id="@+id/usernameTextDrIn"
        android:layout_width="215dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="56dp"
        android:ems="10"
        android:textColor="#FFFFFF"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carRegTextDrIn" />

    <TextView
        android:id="@+id/phoneTextDrIn"
        android:layout_width="215dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="56dp"
        android:ems="10"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usernameTextDrIn" />

    <TextView
        android:id="@+id/emailTextDrIn"
        android:layout_width="215dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="56dp"
        android:ems="10"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phoneTextDrIn" />

    <TextView
        android:id="@+id/availabilityTextDrIn"
        android:layout_width="215dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="56dp"
        android:ems="10"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextDrIn" />

    <TextView
        android:id="@+id/posRateDr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="65dp"
        android:text="100%"
        android:textColor="#00FF00"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/usernameLabelDrIn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/likeButtonDrIn"
        app:layout_constraintVertical_bias="0.021" />

    <TextView
        android:id="@+id/negRateDr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="66dp"
        android:text="100%"
        android:textColor="#FF0000"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/usernameTextDrIn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dislikeButtonDrIn"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>